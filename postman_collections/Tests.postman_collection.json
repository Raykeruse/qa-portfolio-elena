		{
			"name": "Статус код",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Проверка на статус-код 200 + статус сообщения OK\", function () {",
							"    pm.response.to.have.status(200);",
							"    pm.response.to.have.status(\"OK\");",
							"});",
							"",
							"// pm.test(\"Проверка нескольких статус-кодов\", function () {",
							"//     pm.expect(pm.response.code).to.be.oneOf([200, 404]);",
							"// });",
							"",
							"// pm.test(\"Проверка на Статус сообщения - OK\", function () {",
							"//     pm.response.to.have.status(\"OK\");",
							"// });",
							"",
							"pm.test(\"Проверка на Статус-код - 100-й\", function () {",
							"    pm.response.to.be.info;",
							"});",
							"",
							"pm.test(\"Проверка на Статус-код - 200-й\", function () {",
							"    pm.response.to.be.success;",
							"});",
							"",
							"pm.test(\"Проверка на Статус-код - 300-й\", function () {",
							"    pm.response.to.be.redirection;",
							"});",
							"",
							"pm.test(\"Проверка на Статус-код - 400-й и 500-й\", function () {",
							"    pm.response.to.be.error;",
							"});",
							"",
							"pm.test(\"Проверка на Статус-код - 400-й\", function () {",
							"    pm.response.to.be.error;",
							"});",
							"",
							"pm.test(\"Проверка на Статус-код - 500-й\", function () {",
							"    pm.response.to.be.serverError;",
							"});"
						],
